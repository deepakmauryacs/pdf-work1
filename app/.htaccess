# WAMP-safe .htaccess
RewriteEngine On
# Block direct access to storage
RewriteRule ^storage/ - [F,L]

# If mod_headers is enabled, uncomment for extra headers
# <IfModule mod_headers.c>
#   Header always set X-Content-Type-Options "nosniff"
#   Header always set Referrer-Policy "strict-origin-when-cross-origin"
#   Header set Accept-Ranges bytes
#   Header set Content-Security-Policy "default-src 'self'; img-src 'self' data:; script-src 'self' https://cdn.jsdelivr.net; style-src 'self' 'unsafe-inline'; frame-ancestors 'self' https://example.com;"
# </IfModule>
